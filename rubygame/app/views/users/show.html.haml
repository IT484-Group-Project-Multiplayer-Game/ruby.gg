%div{:class => "user_info"}
  %p
    - if current_user
      Signed in as
      
      %strong= @user.name
      %p
        Provider:
        %strong=@user.provider
      %p
        UserID:
        %strong=@user.uid
      %p
        Email Address:
        %strong=@user.email
      
      
        
      = link_to "Sign out", signout_path, id: "sign_out"
    
    